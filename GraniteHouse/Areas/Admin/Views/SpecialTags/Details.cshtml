        <h2 class="text-info">Special Tags Details</h2>
    </div>
</div>

<br /> <br />



<div class="p-4 border">
    <input asp-for="Id" type="hidden" />
    <div class="form-group row">
        <div class="col-2">
            <label asp-for="Name"></label>
        </div>
